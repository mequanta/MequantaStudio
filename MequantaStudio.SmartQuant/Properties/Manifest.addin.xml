    <Extension path="/MonoDevelop/Ide/ProjectTemplates">
        <ProjectTemplate id="SQInstrumentStrategyProject" resource="InstrumentStrategyProject.xpt.xml" />
        <ProjectTemplate id="SQStrategyProject" resource="StrategyProject.xpt.xml" />
        <ProjectTemplate id="SQSellSideStrategyProject" resource="SellSideStrategyProject.xpt.xml" />
        <ProjectTemplate id="SQScenarioProject" resource="ScenarioProject.xpt.xml" />
    </Extension>
    <Extension path="/MonoDevelop/Ide/Commands">
        <Category _name="SmartQuant" id="SmartQuant">
            <Command id="MonoDevelop.SmartQuant.RunCommands.Start" defaultHandler="MonoDevelop.SmartQuant.Commands.StartRunHandler" _label="Start" _description="Start Execution" icon="md-pause-debug" />
            <Command id="MonoDevelop.SmartQuant.RunCommands.Pause" defaultHandler="MonoDevelop.SmartQuant.Commands.PauseRunHandler" shortcut="Control|Break" _label="Pause" _description="Pause Execution" macShortcut="Alt|Meta|P" icon="md-pause-debug" />
            <Command id="MonoDevelop.SmartQuant.RunCommands.Continue" defaultHandler="MonoDevelop.SmartQuant.Commands.ContinueRunHandler" _label="Continue" _description="Continue Execution" icon="md-continue-debug" />
            <Command id="MonoDevelop.SmartQuant.RunCommands.Stop" defaultHandler="MonoDevelop.SmartQuant.Commands.StopRunHandler" _label="Step Over" _description="Stop" shortcut="F10" macShortcut="Shift|Meta|O" icon="md-step-over-debug" />
            <Command id="MonoDevelop.SmartQuant.RunCommands.StepNext" defaultHandler="MonoDevelop.SmartQuant.Commands.StepNextHandler" _label="Step Into" _description="Step Next" shortcut="F11" macShortcut="Shift|Meta|I" icon="md-step-into-debug" />
        </Category>
        <Command id="MequantaStudio.SmartQuant.AccountCommands.Open" _label="Account" defaultHandler="MequantaStudio.SmartQuant.AccountCommandHandler" />
        <Command id="MonoDevelop.SmartQuant.Commands.ImportCommands.ImportInstrumentList" defaultHandler="MonoDevelop.SmartQuant.Commands.ImportInstrumentListHandler" type="array" _label="" />
        <Command id="MonoDevelop.SmartQuant.Commands.ImportCommands.ImportHistoricalList" defaultHandler="MonoDevelop.SmartQuant.Commands.ImportHistoricalListHandler" type="array" _label="" />
        <Command id="MonoDevelop.SmartQuant.Commands.ImportCommands.ImportCsvOrTextFiles" _label="CSV or Text Files..." defaultHandler="MonoDevelop.SmartQuant.Commands.ImportCsvOrTextFileHandler" />
    </Extension>
    <Extension path="/MonoDevelop/Ide/Commands">
        <Command id="MequantaStudio.SmartQuant.InstrumentNodeCommands.AddNew" _label="Add New..." defaultHandler="MequantaStudio.SmartQuant.InstrumentNodeCommandHandler" />
        <Command id="MequantaStudio.SmartQuant.InstrumentNodeCommands.ViewData" _label="Data" defaultHandler="MequantaStudio.SmartQuant.InstrumentNodeCommandHandler" />
        <Command id="MequantaStudio.SmartQuant.InstrumentNodeCommands.Delete" _label="Delete" defaultHandler="MequantaStudio.SmartQuant.InstrumentNodeCommandHandler" />
        <Command id="MequantaStudio.SmartQuant.ProviderCommands.Connect" _label="Connect" defaultHandler="MequantaStudio.SmartQuant.ProviderNodeCommandHandler" />
        <Command id="MequantaStudio.SmartQuant.ProviderCommands.Disconnect" _label="Disconnect" defaultHandler="MequantaStudio.SmartQuant.ProviderNodeCommandHandler" />
        <Command id="MequantaStudio.SmartQuant.PortfolioCommands.AddNew" _label="Add New..." defaultHandler="MequantaStudio.SmartQuant.PortfolioNodeCommandHandler" />
        <Command id="MequantaStudio.SmartQuant.PortfolioCommands.ViewData" _label="View..." defaultHandler="MequantaStudio.SmartQuant.PortfolioNodeCommandHandler" />
        <Command id="MequantaStudio.SmartQuant.PortfolioCommands.Delete" _label="Delete" defaultHandler="MequantaStudio.SmartQuant.PortfolioNodeCommandHandler" />
    </Extension>
    <Extension path="/MonoDevelop/Ide/Commands">
        <Category _name="SmartQuant" id="SmartQuant">
            <Command id="MequantaStudio.SmartQuant.Commands.ShowXwtContentView" _label="Xwt ContentView" defaultHandler="MequantaStudio.SmartQuant.Commands.ShowXwtContentViewHandler" />
            <Command id="MequantaStudio.SmartQuant.Commands.ShowGtkContentView" _label="Gtk ContentView" defaultHandler="MequantaStudio.SmartQuant.Commands.ShowGtkContentViewHandler" />
        </Category>
    </Extension>
    <Extension path="/MonoDevelop/Ide/MainMenu">
        <ItemSet id="SmartQuant" _label="_SmartQuant" insertafter="Search">
            <CommandItem id="MequantaStudio.SmartQuant.AccountCommands.Open" />
            <CommandItem id="MequantaStudio.SmartQuant.Commands.ShowOrderManager" />
            <CommandItem id="MequantaStudio.SmartQuant.Commands.ShowChart" />
            <CommandItem id="MequantaStudio.SmartQuant.Commands.ShowStrategyMonitor" />
            <CommandItem id="MequantaStudio.SmartQuant.Commands.ShowQuoteMonitor" />
            <CommandItem id="MequantaStudio.SmartQuant.Commands.ShowStartPage" />
            <SeparatorItem id="DataManagerSeparator" />
            <CommandItem id="MonoDevelop.SmartQuant.Commands.ShowDataManager" />
            <SeparatorItem id="TestSeparator" />
            <CommandItem id="MequantaStudio.SmartQuant.Commands.ShowXwtContentView" />
            <CommandItem id="MequantaStudio.SmartQuant.Commands.ShowGtkContentView" />
        </ItemSet>
    </Extension>

    <ExtensionPoint path = "/MonoDevelop/Ide/ContextMenu/InstrumentPad" name = "Instrument pad context menu">
        <Description>Context menu for nodes in the instrument pad.</Description>
        <ExtensionNodeSet id="MonoDevelop.Components.Commands.ItemSet"/>
        <ConditionType id="ItemType" type="MonoDevelop.Projects.Extensions.ItemTypeCondition">
            <Description>Type of the item. If no namespace is provided, MonoDevelop.Projects is assumed.</Description>
        </ConditionType>
    </ExtensionPoint>

    <ExtensionPoint path="/MequantaStudio/SmartQuant/ContextMenu/InstrumentPad/InstrumentFolderNode">
